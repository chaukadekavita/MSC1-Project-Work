using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace ConsoleApplication30
{
    class Program
    {
        static void Main(string[] args)
        {
            int x = 20, y = 10;
            var result = x > y ? x : y;
            Console.WriteLine(result);
            Console.ReadKey();
        }
    }
}
output
true
